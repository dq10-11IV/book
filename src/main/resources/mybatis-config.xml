<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">  
<configuration>  
    <typeAliases>  
        <typeAlias alias="User" type="com.turtledove.bookdrift.domain.entity.User"/>
        <typeAlias alias="Book" type="com.turtledove.bookdrift.domain.entity.Book"/>
        <typeAlias alias="Label" type="com.turtledove.bookdrift.domain.entity.Label" />
        <typeAlias alias="UserBookAss" type="com.turtledove.bookdrift.domain.entity.UserBookAss"/>
        <typeAlias alias="UserLabelAss" type="com.turtledove.bookdrift.domain.entity.UserLabelAss"/>  
        <typeAlias alias="UserInfo"  type="com.turtledove.bookdrift.domain.entity.UserInfo"/>
    </typeAliases>  
    <mappers>  
        <mapper resource="sqlMapper/UserDao.xml" />
        <mapper resource="sqlMapper/BookDao.xml"/>  
        <mapper resource="sqlMapper/LabelDao.xml"/>
        <mapper resource="sqlMapper/UserBookAssDao.xml"/>
        <mapper resource="sqlMapper/UserLabelAssDao.xml"/>
        <mapper resource="sqlMapper/QueryDao.xml"/>
    </mappers>  
</configuration>    