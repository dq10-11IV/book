<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.turtledove.bookdrift.infrastruct.dao.UserLabelAssDao">
	<resultMap type="UserLabelAss" id="result">
		<id property="Id" column="USER_LABEL_ID" />
		<result property="userId" column="USER_ID" />
		<result property="labelId" column="LABEL_ID" />
		<result property="createDate" column="CREATE_DATE" />
		<result property="lastUpdateDate" column="LAST_UPDATE_DATE" />
	</resultMap>
	<sql id="sql_select">
		SELECT
		USER_LABEL_ID,
		USER_ID,
		LABEL_ID,
		CREATE_DATE,
		LAST_UPDATE_DATE
		FROM bd_user_label
	</sql>
	<select id="findById" parameterType="Integer" resultMap="result">
		<include refid="sql_select" />
		WHERE USER_LABEL_ID = #{Id};
	</select>
	<insert id="insert" parameterType="UserLabelAss"
		useGeneratedKeys="true" keyProperty="Id">
		INSERT INTO bd_user_label(USER_ID,LABEL_ID,CREATE_DATE,LAST_UPDATE_DATE)
		VALUE(#{userId},#{labelId},#{createDate},#{lastUpdateDate});
	</insert>
</mapper>